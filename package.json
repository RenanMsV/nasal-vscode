{
    "name": "nasal-lang",
    "displayName": "Nasal-lang",
    "description": "Nasal language support for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "nasal",
            "aliases": ["Nasal", "nasal"],
            "extensions": [".nas"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nasal",
            "scopeName": "source.nas",
            "path": "./syntaxes/nasal.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "nasal",
                "scopeName": "source.nas",
                "path": "./snippets/nasal.json"
            },
            {
                "language": "nasal",
                "scopeName": "source.nas",
                "path": "./snippets/nasal.fg.json"
            }
        ]
    }
}