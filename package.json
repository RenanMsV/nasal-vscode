{
    "name": "nasal-lang",
    "displayName": "Nasal-lang",
    "description": "Nasal language support for Visual Studio Code",
    "version": "0.0.5",
    "publisher": "Renan-MsV",
    "author": {
        "name": "RenanMsV https://github.com/RenanMsV"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "GPL",
    "bugs": {
        "url": "https://github.com/RenanMsV/nasal-vscode/issues",
        "email": "none@none.none"
    },
    "repository": {
        "type": "git",
        "url" : "https://github.com/RenanMsV/nasal-vscode.git"
    },
    "homepage": "https://github.com/RenanMsV/nasal-vscode",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "nasal",
        "flightgear"
    ],
    "contributes": {
        "languages": [{
            "id": "nasal",
            "aliases": ["Nasal", "nasal"],
            "extensions": [".nas"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nasal",
            "scopeName": "source.nas",
            "path": "./syntaxes/nasal.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "nasal",
                "scopeName": "source.nas",
                "path": "./snippets/nasal.json"
            },
            {
                "language": "nasal",
                "scopeName": "source.nas",
                "path": "./snippets/nasal.fg.json"
            }
        ]
    }
}